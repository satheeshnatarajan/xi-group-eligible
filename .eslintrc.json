{
  "env": {
    "browser": true,
    "jest": true,
    "es6": true
  },
  "extends": ["plugin:react/recommended"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-use-before-define": ["warn", { "functions": true, "classes": true }],
    "no-param-reassign": "warn",
    "no-console": "warn",
    "no-eval": "error",
    "no-unused-vars": "off"
  }
}
